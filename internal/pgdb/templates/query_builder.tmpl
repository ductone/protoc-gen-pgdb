func  (x *{{ .OpsTypeName }}) ObjectContainsRaw(obj exp.Expression) exp.Expression {
	idExp := exp.NewIdentifierExpression("", x.tableName, x.column)
	return exp.NewLiteralExpression("(? @> ?)", idExp, obj)
}

func  (x *{{ .OpsTypeName }}) ObjectContains(obj interface{}) (exp.Expression, error) {
	var err error
	var data []byte

	pm, ok := obj.(proto.Message)
	if ok {
		data, err = protojson.Marshal(pm)
	} else {
		data, err = json.Marshal(obj)
	}
	if err != nil {
		return nil, err
	}

	return x.ObjectContainsRaw(exp.NewLiteralExpression("?::jsonb", string(data))), nil
}
{{end}}

